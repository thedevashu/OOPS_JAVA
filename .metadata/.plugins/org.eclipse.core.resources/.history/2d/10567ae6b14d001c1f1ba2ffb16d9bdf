package study.javaIO;

public class Student {

	private String name;
	private int age;
	private int[] marks;
	
	public Student() {
		name=null;
		age=0;
		marks=null;
	}
	public Student(String name,int age,int []marks) {
		
	}
	
	public void setName(String name) {
		this.name =name;
	}
	public String getName() {
		return this.name;
	}
}
