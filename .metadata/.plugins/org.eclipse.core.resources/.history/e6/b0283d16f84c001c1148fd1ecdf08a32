package study.practice;

public class MonthlyExpenseUser {

	public static void main(String[] args) {
		MonthlyExpense m =new MonthlyExpense();
		m.addNewExpens(new ExpenseDetails("ABCD", 0, new MyDate(10, 11, 2021)));
		m.addNewExpens(new ExpenseDetails("EFGH", 0, new MyDate(12, 11, 2021)));
		m.addNewExpens(new ExpenseDetails("IJKL", 599, new MyDate(16, 10, 2021)));
		m.addNewExpens(new ExpenseDetails("MNOP", 0, new MyDate(19, 11, 2021)));
		m.addNewExpens(new ExpenseDetails("QRST", 0, new MyDate(11, 11, 2021)));
		
		m.showAll();
		System.out.println(m.getTotalExpenditur());

	}

}
